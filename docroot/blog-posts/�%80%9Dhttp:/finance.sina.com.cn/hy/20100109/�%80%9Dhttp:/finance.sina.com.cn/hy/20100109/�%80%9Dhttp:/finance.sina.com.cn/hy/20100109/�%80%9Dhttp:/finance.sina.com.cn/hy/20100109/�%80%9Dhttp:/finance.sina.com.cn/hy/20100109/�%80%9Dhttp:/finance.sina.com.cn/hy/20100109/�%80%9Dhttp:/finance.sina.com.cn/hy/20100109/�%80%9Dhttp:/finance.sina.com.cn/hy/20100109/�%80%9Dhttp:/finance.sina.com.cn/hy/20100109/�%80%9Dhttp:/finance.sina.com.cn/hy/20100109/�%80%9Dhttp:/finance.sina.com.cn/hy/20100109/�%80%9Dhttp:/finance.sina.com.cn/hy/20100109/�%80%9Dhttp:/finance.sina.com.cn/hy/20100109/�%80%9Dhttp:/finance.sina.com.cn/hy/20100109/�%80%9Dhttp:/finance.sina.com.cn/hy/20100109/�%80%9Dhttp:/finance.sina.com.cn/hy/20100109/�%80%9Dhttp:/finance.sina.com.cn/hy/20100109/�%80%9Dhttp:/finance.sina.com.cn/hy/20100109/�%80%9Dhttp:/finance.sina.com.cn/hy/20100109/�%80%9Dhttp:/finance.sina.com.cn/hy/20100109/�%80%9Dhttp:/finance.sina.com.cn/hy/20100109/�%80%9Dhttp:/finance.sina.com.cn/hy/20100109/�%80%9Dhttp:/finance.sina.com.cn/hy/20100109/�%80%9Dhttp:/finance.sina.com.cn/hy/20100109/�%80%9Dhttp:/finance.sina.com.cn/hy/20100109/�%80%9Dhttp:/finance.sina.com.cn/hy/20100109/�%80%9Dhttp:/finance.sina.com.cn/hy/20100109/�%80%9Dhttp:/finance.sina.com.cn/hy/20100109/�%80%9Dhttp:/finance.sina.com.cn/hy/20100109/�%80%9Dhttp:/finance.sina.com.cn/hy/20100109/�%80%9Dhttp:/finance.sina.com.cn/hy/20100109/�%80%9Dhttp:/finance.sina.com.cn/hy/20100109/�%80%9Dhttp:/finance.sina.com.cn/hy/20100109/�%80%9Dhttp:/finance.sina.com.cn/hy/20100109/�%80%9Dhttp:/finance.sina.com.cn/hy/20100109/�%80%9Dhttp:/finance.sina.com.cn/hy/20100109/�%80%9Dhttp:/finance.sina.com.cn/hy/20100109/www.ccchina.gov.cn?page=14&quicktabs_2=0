    <script type="text/javascript">
      function equalHeight(group) {
	  tallest = 0;
	  group.each(function() {
		thisHeight = $(this).height();
		if(thisHeight > tallest) {
			tallest = thisHeight;
		}
	  });
	  group.height(tallest);
      }

     $(document).ready(function() {
	  equalHeight($(".feature"));
     });
   </script>

 
