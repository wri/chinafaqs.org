    &lt;div class=&quot;field-items&quot;&gt;
            &lt;div class=&quot;field-item odd&quot;&gt;
                    &lt;div class=&quot;filefield-file&quot;&gt;&lt;img class=&quot;filefield-icon field-icon-application-pdf&quot;  alt=&quot;application/pdf icon&quot; src=&quot;http://www.chinafaqs.org/sites/all/modules/filefield/icons/application-pdf.png&quot; /&gt;&lt;a href=&quot;http://www.chinafaqs.org/files/chinainfo/Herberg_USCC%20testimony_26%202012.pdf&quot; type=&quot;application/pdf; length=75401&quot;&gt;Herberg_USCC testimony_26 2012.pdf&lt;/a&gt;&lt;/div&gt;        &lt;/div&gt;
